DATABASE_URL=postgresql://user:password@host:5432/gym_tracker?sslmode=require
PORT=3001

# Comma-separated list of allowed origins for CORS (e.g. http://localhost:3000,https://myapp.com)
# In development: if omitted, localhost origins (ports 3000-3999) are allowed
# In production (NODE_ENV=production): if omitted, all origins are blocked
ALLOWED_ORIGINS=

# WorkOS OAuth 2.1 SSO (required for multi-user auth)
WORKOS_API_KEY=sk_...
WORKOS_CLIENT_ID=client_...
BASE_URL=http://localhost:3001

# Dev only: skip auth and use this user ID
DEV_USER_ID=1
